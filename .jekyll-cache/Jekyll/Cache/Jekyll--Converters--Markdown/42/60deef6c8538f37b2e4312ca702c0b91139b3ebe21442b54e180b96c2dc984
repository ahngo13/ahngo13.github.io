                        &lt;/p&gt;
                    &lt;/form&gt;
                &lt;/article&gt;
                `</span><span class="p">;</span>
            <span class="si">}</span>
        }
        
        //reducer
        function reducer(state, action)<span class="si">{</span>
            
            <span class="c1">//ì´ˆê¸° stateê°€ undefinedì¼ ê²½ìš°</span>
            <span class="k">if</span><span class="p">(</span><span class="nx">state</span> <span class="o">===</span> <span class="kc">undefined</span><span class="p">){</span>
                <span class="k">return</span> <span class="p">{</span>
                    <span class="na">max_id</span><span class="p">:</span><span class="mi">3</span><span class="p">,</span>
                    <span class="na">mode</span><span class="p">:</span><span class="dl">"</span><span class="s2">create</span><span class="dl">"</span><span class="p">,</span>
                    <span class="na">selected_id</span><span class="p">:</span><span class="mi">1</span><span class="p">,</span>
                    <span class="na">contents</span> <span class="p">:</span> <span class="p">[</span>
                        <span class="p">{</span><span class="na">id</span><span class="p">:</span><span class="mi">1</span><span class="p">,</span> <span class="na">title</span><span class="p">:</span><span class="dl">'</span><span class="s1">HTML</span><span class="dl">'</span><span class="p">,</span><span class="na">desc</span><span class="p">:</span><span class="dl">'</span><span class="s1">HTML is ..</span><span class="dl">'</span><span class="p">},</span>
                        <span class="p">{</span><span class="na">id</span><span class="p">:</span><span class="mi">2</span><span class="p">,</span> <span class="na">title</span><span class="p">:</span><span class="dl">'</span><span class="s1">CSS</span><span class="dl">'</span><span class="p">,</span><span class="na">desc</span><span class="p">:</span><span class="dl">'</span><span class="s1">CSS is ..</span><span class="dl">'</span><span class="p">},</span>
                        <span class="p">{</span><span class="na">id</span><span class="p">:</span><span class="mi">3</span><span class="p">,</span> <span class="na">title</span><span class="p">:</span><span class="dl">'</span><span class="s1">REACT</span><span class="dl">'</span><span class="p">,</span><span class="na">desc</span><span class="p">:</span><span class="dl">'</span><span class="s1">REACT is ..</span><span class="dl">'</span><span class="p">},</span>
                    <span class="p">]</span>
                <span class="p">}</span>
            <span class="p">}</span>
            <span class="kd">var</span> <span class="nx">newState</span><span class="p">;</span>
            <span class="c1">//ì¡°íšŒ</span>
            <span class="k">if</span><span class="p">(</span><span class="nx">action</span><span class="p">.</span><span class="nx">type</span> <span class="o">===</span> <span class="dl">'</span><span class="s1">SELECT</span><span class="dl">'</span><span class="p">){</span>
                <span class="nx">newState</span> <span class="o">=</span> <span class="nb">Object</span><span class="p">.</span><span class="nx">assign</span><span class="p">({},</span> <span class="nx">state</span><span class="p">,</span> 
                <span class="p">{</span><span class="na">selected_id</span><span class="p">:</span><span class="nx">action</span><span class="p">.</span><span class="nx">id</span><span class="p">,</span>
                    <span class="na">mode</span><span class="p">:</span> <span class="dl">'</span><span class="s1">read</span><span class="dl">'</span>
                <span class="p">});</span>
            <span class="c1">//ì¶”ê°€</span>
            <span class="p">}</span><span class="k">else</span> <span class="k">if</span><span class="p">(</span><span class="nx">action</span><span class="p">.</span><span class="nx">type</span> <span class="o">===</span> <span class="dl">'</span><span class="s1">CREATE</span><span class="dl">'</span><span class="p">){</span>
                <span class="kd">const</span> <span class="nx">newMaxId</span> <span class="o">=</span> <span class="nx">state</span><span class="p">.</span><span class="nx">max_id</span> <span class="o">+</span> <span class="mi">1</span><span class="p">;</span>
                <span class="kd">const</span> <span class="nx">newContents</span> <span class="o">=</span> <span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">.</span><span class="nx">concat</span><span class="p">();</span>
                <span class="nx">newContents</span><span class="p">.</span><span class="nx">push</span><span class="p">({</span><span class="na">id</span><span class="p">:</span> <span class="nx">newMaxId</span><span class="p">,</span> <span class="na">title</span><span class="p">:</span> <span class="nx">action</span><span class="p">.</span><span class="nx">title</span><span class="p">,</span> <span class="na">desc</span><span class="p">:</span> <span class="nx">action</span><span class="p">.</span><span class="nx">desc</span><span class="p">})</span>
                <span class="nx">newState</span> <span class="o">=</span> <span class="nb">Object</span><span class="p">.</span><span class="nx">assign</span><span class="p">({},</span> <span class="nx">state</span><span class="p">,</span> <span class="p">{</span>
                    <span class="na">max_id</span><span class="p">:</span><span class="nx">newMaxId</span><span class="p">,</span>
                    <span class="na">contents</span><span class="p">:</span> <span class="nx">newContents</span><span class="p">,</span>
                    <span class="na">mode</span><span class="p">:</span><span class="dl">'</span><span class="s1">read</span><span class="dl">'</span>
                <span class="p">})</span>
            <span class="c1">//ì‚­ì œ</span>
            <span class="p">}</span><span class="k">else</span> <span class="k">if</span><span class="p">(</span><span class="nx">action</span><span class="p">.</span><span class="nx">type</span> <span class="o">===</span> <span class="dl">'</span><span class="s1">DELETE</span><span class="dl">'</span><span class="p">){</span>
                <span class="kd">const</span> <span class="nx">newContents</span> <span class="o">=</span> <span class="p">[];</span>
                <span class="kd">let</span> <span class="nx">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
                <span class="k">while</span> <span class="p">(</span><span class="nx">i</span><span class="o">&lt;</span><span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">.</span><span class="nx">length</span><span class="p">){</span>
                    <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="dl">'</span><span class="s1">content[i]</span><span class="dl">'</span><span class="o">+</span><span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">[</span><span class="nx">i</span><span class="p">].</span><span class="nx">id</span><span class="p">);</span>
                    <span class="k">if</span><span class="p">(</span><span class="nx">state</span><span class="p">.</span><span class="nx">selected_id</span> <span class="o">!==</span> <span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">[</span><span class="nx">i</span><span class="p">].</span><span class="nx">id</span><span class="p">){</span>
                        <span class="nx">newContents</span><span class="p">.</span><span class="nx">push</span><span class="p">(</span>
                            <span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">[</span><span class="nx">i</span><span class="p">]</span>
                        <span class="p">);</span>
                    <span class="p">}</span>
                    <span class="nx">i</span> <span class="o">=</span> <span class="nx">i</span><span class="o">+</span><span class="mi">1</span><span class="p">;</span>
                <span class="si">}</span>

                newState = Object.assign(<span class="si">{}</span>,state,<span class="si">{</span>
                    <span class="nx">contents</span><span class="p">:</span><span class="nx">newContents</span><span class="p">,</span>
                    <span class="nx">mode</span><span class="p">:</span><span class="dl">'</span><span class="s1">welcome</span><span class="dl">'</span>
                <span class="si">}</span>)
            //ëª¨ë“œ ë³€ê²½ (ì¶”ê°€, ìˆ˜ì •í¼ ë…¸ì¶œìš©)
            }else if(action.type === 'CHANGE_MODE')<span class="si">{</span>
                <span class="nx">newState</span> <span class="o">=</span> <span class="nb">Object</span><span class="p">.</span><span class="nx">assign</span><span class="p">({},</span><span class="nx">state</span><span class="p">,{</span>
                    <span class="na">mode</span><span class="p">:</span><span class="nx">action</span><span class="p">.</span><span class="nx">mode</span>
                <span class="p">})</span>
            <span class="c1">//ìˆ˜ì •</span>
            <span class="si">}</span>else if(action.type === 'UPDATE')<span class="si">{</span>
                <span class="kd">const</span> <span class="nx">newContents</span> <span class="o">=</span> <span class="p">[];</span>
                <span class="kd">let</span> <span class="nx">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
                <span class="k">while</span> <span class="p">(</span><span class="nx">i</span><span class="o">&lt;</span><span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">.</span><span class="nx">length</span><span class="p">){</span>
                    <span class="k">if</span><span class="p">(</span><span class="nx">state</span><span class="p">.</span><span class="nx">selected_id</span> <span class="o">!==</span> <span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">[</span><span class="nx">i</span><span class="p">].</span><span class="nx">id</span><span class="p">){</span>
                        <span class="nx">newContents</span><span class="p">.</span><span class="nx">push</span><span class="p">(</span>
                            <span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">[</span><span class="nx">i</span><span class="p">]</span>
                        <span class="p">);</span>
                    <span class="p">}</span><span class="k">else</span><span class="p">{</span>
                        <span class="nx">newContents</span><span class="p">.</span><span class="nx">push</span><span class="p">({</span>
                            <span class="na">id</span><span class="p">:</span><span class="nx">state</span><span class="p">.</span><span class="nx">selected_id</span><span class="p">,</span>
                            <span class="na">title</span><span class="p">:</span> <span class="nx">action</span><span class="p">.</span><span class="nx">title</span><span class="p">,</span>
                            <span class="na">contents</span><span class="p">:</span> <span class="nx">action</span><span class="p">.</span><span class="nx">contents</span><span class="p">,</span>
                            <span class="na">desc</span><span class="p">:</span> <span class="nx">action</span><span class="p">.</span><span class="nx">desc</span><span class="p">,</span>
                        <span class="p">});</span>
                    <span class="p">}</span>
                    <span class="nx">i</span> <span class="o">=</span> <span class="nx">i</span><span class="o">+</span><span class="mi">1</span><span class="p">;</span>

                    <span class="nx">newState</span> <span class="o">=</span> <span class="nb">Object</span><span class="p">.</span><span class="nx">assign</span><span class="p">({},</span><span class="nx">state</span><span class="p">,{</span>
                        <span class="na">contents</span><span class="p">:</span><span class="nx">newContents</span><span class="p">,</span>
                        <span class="na">mode</span><span class="p">:</span><span class="dl">'</span><span class="s1">welcome</span><span class="dl">'</span>
                    <span class="p">})</span>
                <span class="si">}</span>
            }
            console.log(state, action, newState);
            return newState;
        }

        //store ìƒì„±
        const store = Redux.createStore(reducer);
        //ìƒíƒœê°’ì— ë”°ë¼ ë Œë”ë§ í•´ì¤„ í•¨ìˆ˜ ì¶”ê°€
        store.subscribe(article);
        store.subscribe(TOC);
        //ì´ˆê¸°ì— í™”ë©´ ê·¸ë¦¬ê¸°
        subject();
        TOC();
        control();
        article();

    <span class="p">&lt;/</span><span class="nt">script</span><span class="p">&gt;</span>
    
  
   
<span class="p">&lt;/</span><span class="nt">body</span><span class="p">&gt;</span>
<span class="p">&lt;/</span><span class="nt">html</span><span class="p">&gt;</span>
</code></pre></div></div>

