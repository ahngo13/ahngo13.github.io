        <span class="si">}</span>
        //TOC ì˜ì—­
        function TOC()<span class="si">{</span>
            <span class="c1">//store ê°’ ê°€ì ¸ì˜¤ê¸°</span>
            <span class="kd">const</span> <span class="nx">state</span> <span class="o">=</span> <span class="nx">store</span><span class="p">.</span><span class="nx">getState</span><span class="p">();</span>
            <span class="kd">let</span> <span class="nx">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
            <span class="kd">let</span> <span class="nx">liTags</span> <span class="o">=</span> <span class="dl">''</span><span class="p">;</span>
            <span class="c1">//ë°˜ë³µë¬¸ì„ í†µí•´ contents ê°¯ìˆ˜ë§Œí¼ li íƒœê·¸ ìƒì„±</span>
            <span class="k">while</span><span class="p">(</span><span class="nx">i</span><span class="o">&lt;</span><span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">.</span><span class="nx">length</span><span class="p">){</span>
                <span class="c1">// aíƒœê·¸ í´ë¦­ì‹œ SELECT action í˜¸ì¶œ (idê°’ í•˜ë‚˜ë§Œ ë„˜ê²¨ì£¼ë©´ ë¨)</span>
                <span class="nx">liTags</span> <span class="o">+=</span> <span class="s2">`
                    &lt;li&gt;
                        &lt;a onclick="event.preventDefault();
                        const action = {type:'SELECT', id:</span><span class="p">${</span><span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">[</span><span class="nx">i</span><span class="p">].</span><span class="nx">id</span><span class="p">}</span><span class="s2">}
                        store.dispatch(action)"
                            href="</span><span class="p">${</span><span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">[</span><span class="nx">i</span><span class="p">].</span><span class="nx">id</span><span class="p">}</span><span class="s2">"&gt;
                            </span><span class="p">${</span><span class="nx">state</span><span class="p">.</span><span class="nx">contents</span><span class="p">[</span><span class="nx">i</span><span class="p">].</span><span class="nx">title</span><span class="p">}</span><span class="s2">
                        &lt;/a&gt;
                    &lt;/li&gt;
                `</span><span class="p">;</span>
                <span class="nx">i</span><span class="o">=</span> <span class="nx">i</span><span class="o">+</span><span class="mi">1</span><span class="p">;</span>
            <span class="p">}</span>
            <span class="nb">document</span><span class="p">.</span><span class="nx">querySelector</span><span class="p">(</span><span class="dl">'</span><span class="s1">#TOC</span><span class="dl">'</span><span class="p">).</span><span class="nx">innerHTML</span> <span class="o">=</span> <span class="s2">`
            &lt;nav&gt;
                &lt;ol&gt;
                    </span><span class="p">${</span><span class="nx">liTags</span><span class="p">}</span><span class="s2">
                &lt;/ol&gt;
            &lt;/nav&gt;
            `</span><span class="p">;</span>
        <span class="si">}</span>
        //control ì˜ì—­
        function control()<span class="si">{</span>
            <span class="c1">//create, delete, update ë²„íŠ¼ ë° aíƒœê·¸ ê·¸ë¦¬ê¸°</span>
            <span class="c1">//createì™€ updateëŠ” í•˜ë‹¨ì— ì…ë ¥ form ë…¸ì¶œí•˜ë„ë¡ CHANGE_MODE action type, mode ê°ê° ì„¸íŒ…</span>
            <span class="c1">//deleteëŠ” DELETE action type ì„¸íŒ…</span>
            <span class="nb">document</span><span class="p">.</span><span class="nx">querySelector</span><span class="p">(</span><span class="dl">'</span><span class="s1">#control</span><span class="dl">'</span><span class="p">).</span><span class="nx">innerHTML</span><span class="o">=</span><span class="s2">`
            &lt;ul&gt;
                &lt;li&gt;&lt;a onclick="event.preventDefault();
                    store.dispatch({type:'CHANGE_MODE',
                    mode:'create'
                })
                    " href="/create"&gt;create&lt;/a&gt;&lt;/li&gt;
                &lt;li&gt;&lt;input type="button" onclick="
                    store.dispatch({
                        type : 'DELETE'
                    })" value="delete"&gt;&lt;/li&gt;
                &lt;li&gt;&lt;a onclick="event.preventDefault();
                store.dispatch({type:'CHANGE_MODE',
                mode:'update'
                })
                " href="/update"&gt;update&lt;/a&gt;&lt;/li&gt;
            &lt;/ul&gt;
            `</span><span class="p">;</span>
        <span class="si">}</span>
        //article ì˜ì—­
        function article()<span class="si">{</span>
            <span class="c1">//store ê°ì²´ ê°€ì ¸ì˜¤ê¸°</span>
            <span class="kd">const</span> <span class="nx">state</span> <span class="o">=</span> <span class="nx">store</span><span class="p">.</span><span class="nx">getState</span><span class="p">();</span>
            <span class="c1">//create modeì¼ ê²½ìš°</span>
            <span class="k">if</span><span class="p">(</span><span class="nx">state</span><span class="p">.</span><span class="nx">mode</span> <span class="o">===</span> <span class="dl">'</span><span class="s1">create</span><span class="dl">'</span><span class="p">){</span>
                <span class="c1">//í•˜ë‹¨ì— ì…ë ¥ í¼ ìƒì„± submit ë²„íŠ¼ì„ í†µí•´ CREATE type action í˜¸ì¶œ</span>
                <span class="nb">document</span><span class="p">.</span><span class="nx">querySelector</span><span class="p">(</span><span class="dl">'</span><span class="s1">#article</span><span class="dl">'</span><span class="p">).</span><span class="nx">innerHTML</span><span class="o">=</span><span class="s2">`
                &lt;article&gt;
                    &lt;form onsubmit="event.preventDefault();
                        const _title = this.title.value;
                        const _desc = this.desc.value;
                        store.dispatch({
                            type: 'CREATE',
                            title: _title,
                            desc: _desc
                        })
                    "&gt;
                        &lt;p&gt;
                            &lt;input type="text" name="title"
                            placeholder="title"&gt;&lt;br&gt;
