	<span class="o">/</span><span class="n">tmp</span><span class="o">/</span><span class="n">sandbox297177109</span><span class="o">/</span><span class="n">prog</span><span class="o">.</span><span class="k">go</span><span class="o">:</span><span class="m">9</span> <span class="o">+</span><span class="m">0x9f</span>

